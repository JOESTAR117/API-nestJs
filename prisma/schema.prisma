generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Book {
  id          String @id @default(uuid())
  title       String
  description String
  bar_code    String @unique

  @@map("bocks")
}
